{
  "name": "@outblock/noah-business-api-mcp",
  "version": "0.1.11",
  "description": "Noah Business API tools for Model Context Protocol (MCP)",
  "author": {
    "name": "Outblock",
    "url": "https://outblock.io",
    "github": "https://github.com/outblock"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outblock/noah-business-api-mcp.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "noah-mcp": "dist/bin/noah-mcp.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  },
  "keywords": [
    "noah",
