    "start:sse": "node dist/index.js --sse",
    "dev": "bun src/bin/noah-mcp.ts dev src/index.ts",
    "inspect": "bun src/bin/noah-mcp.ts inspect src/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/node": "^22.13.5",
    "@types/yargs": "^17.0.33",
    "bun-types": "^1.2.10",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "execa": "^9.5.2",
    "fastmcp": "^1.21.0",
    "yargs": "^17.7.2",
    "zod": "^3.24.1"
